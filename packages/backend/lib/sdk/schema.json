    "components": {
        "schemas": {
            "AuthStateSer": {
                "type": "object",
                "properties": {
                    "user": {
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/UserSer"
                            }
                        ],
                        "readOnly": true
                    }
                },
                "required": [
                    "user"
                ]
            },
            "LoginSerRequest": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string",
                        "minLength": 1,
                        "description": "Required. 150 characters or fewer. Letters, digits and @/./+/-/_ only.",
                        "pattern": "^[\\w.@+-]+$",
                        "maxLength": 150
                    },
                    "password": {
                        "type": "string",
                        "minLength": 8,
                        "pattern": "^[\\w\\d`\\-=!@#$%^&*()_+[\\]{}():;\\'\",<.>/?\\\\|]{8,32}$",
                        "maxLength": 32
                    }
                },
                "required": [
                    "password",
                    "username"
                ]
            },
            "PatchedUserSerRequest": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string",
                        "minLength": 1,
